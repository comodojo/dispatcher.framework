{
    "name": "comodojo/dispatcher",
    "description": "REST services microframework",
    "license": "GPL-3.0+",
    "type": "project",
    "keywords": [
        "REST",
        "php",
        "rest services",
        "lframework",
        "comodojo"
    ],
    "homepage": "http://dispatcher.comodojo.org",
     "support": {
        "issues": "https://github.com/comodojo/dispatcher.framework/issues"
    },
    "authors": [
        {
            "name": "comodojo",
            "email": "info@comodojo.org"
        }
    ],
     "autoload": {
        "files": [
            "/lib/comodojo/debug.php",
            "/lib/spyc/Spyc.php"
        ],
        "psr-4": {
            "comodojo\\Exception\\": "lib/comodojo/Exception",
            "comodojo\\ObjectRequest\\": "lib/comodojo/ObjectRequest",
            "comodojo\\ObjectRoutingTable\\": "lib/comodojo/ObjectRoutingTable",
            "comodojo\\ObjectRoute\\": "lib/comodojo/ObjectRoute",
            "comodojo\\ObjectResult\\": "lib/comodojo/ObjectResult",
            "comodojo\\": "lib/comodojo"
        },
        "psr-0": { "comodojo\\composer": "" }
    },
    "scripts": {
        "post-root-package-install": [
            "comodojo\\composer\\DispatcherInstallActions::postRootInstall"
        ],
        "post-update-cmd": [
            "comodojo\\composer\\DispatcherInstallActions::postUpdate"
        ]
    },
    "minimum-stability": "stable",
    "require": {

    }
